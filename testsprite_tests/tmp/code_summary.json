                "openapi": "3.0.0",
                "info": {
                    "title": "Shop API",
                    "version": "1.0.0"
                },
                "paths": {
                    "/api/shop": {
                        "get": {
                            "summary": "Search shop products",
                            "responses": {
                                "200": {
                                    "description": "Success"
                                }
                            }
                        }
                    }
                }
            }
        }
    ]
