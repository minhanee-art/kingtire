                "vite.config.js"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "info": {
                    "title": "Inventory API",
                    "version": "1.0.0"
                },
                "paths": {
                    "/api/inventory": {
                        "get": {
                            "summary": "Fetch inventory data",
                            "responses": {
                                "200": {
                                    "description": "Success"
                                }
                            }
                        }
                    }
                }
            }
        },
        {
